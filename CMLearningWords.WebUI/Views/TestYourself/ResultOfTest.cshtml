@model IEnumerable<CreatedTestYourselfViewModel>
@{
    ViewData["Title"] = "ResultsOfTest";
    Layout = "~/Views/Shared/_Layout.cshtml";
    List<CreatedTestYourselfViewModel> listOfResults = (List<CreatedTestYourselfViewModel>)Model.ToList();
}

<h2>ResultsOfTest</h2>

@Html.AntiForgeryToken()
<input type="hidden" class="validation" asp-validation-summary="ModeOnly">
@for (int i = 0; i < listOfResults.Count(); i++)
{
    if (listOfResults[i].MadeMistake == true)
    {
        <span><strong>+</strong></span><br />
    }
    else
    {
        <span>+</span><br />

    }
}